{
    "spec_version": "1.0",
    "indicators": [{
        "id": "P90_ResponseTime",
        "source": "Dynatrace",
        "query": {
            "timeseriesId": "com.dynatrace.builtin:service.responsetime",
            "aggregation": "p90",
            "tags": [ {
                "context": "CONTEXTLESS",
                "key": "environment",
                "value": "Staging"
              },
              {
              "context": "CONTEXTLESS",
              "key": "service",
              "value": "SampleNodeJsService"
              }
            ]
        },
        "grading": {
            "type": "Threshold",
            "thresholds": {
                "upperSevere": 1000000,
                "upperWarning": 500000,
                "lowerWarning ": 0,
                "lowerSevere": 0
            },
            "metricScore": 25
        }
    }, {
        "id": "AVG_ResponseTime",
        "source": "Dynatrace",
        "query": {
            "timeseriesId": "com.dynatrace.builtin:service.responsetime",
            "aggregation": "avg",
            "tags": [ {
                "context": "CONTEXTLESS",
                "key": "environment",
                "value": "Staging"
              },
              {
              "context": "CONTEXTLESS",
              "key": "service",
              "value": "SampleNodeJsService"
              }
            ]
        },
        "grading": {
            "type": "Threshold",
            "thresholds": {
                "upperSevere": 1000000,
                "upperWarning": 500000,
                "lowerWarning ": 0,
                "lowerSevere": 0
            },
            "metricScore": 25
        }
    }, {
        "id": "AVG_FailureRate",
        "source": "Dynatrace",
        "query": {
            "timeseriesId": "com.dynatrace.builtin:service.failurerate",
            "aggregation": "avg",
            "tags": [ {
                "context": "CONTEXTLESS",
                "key": "environment",
                "value": "Staging"
              },
              {
              "context": "CONTEXTLESS",
              "key": "service",
              "value": "SampleNodeJsService"
              }
            ]
        },
        "grading": {
            "type": "Threshold",
            "thresholds": {
                "upperSevere": 10,
                "upperWarning": 1,
                "lowerWarning ": 0,
                "lowerSevere": 0
            },
            "metricScore": 50
        }
    }
    ],
    "objectives": {
        "pass": 100,
        "warning": 25
    }
}